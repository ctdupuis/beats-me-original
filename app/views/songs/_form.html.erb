<%= form_for @song do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :length %>
    <%= f.text_field :length %>

    <%= f.label :artist %>
    <%= f.text_field :artist_id %>

    <%= f.collection_select :genre_id, Genre.all, :id, :name %>

    <%= f.submit "Create Song" %>
<% end %>